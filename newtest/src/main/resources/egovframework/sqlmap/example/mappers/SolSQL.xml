<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="egovframework.example.sample.service.impl.SolMapper">
	<insert id="insPart" parameterType="participant">
		insert into participant (competition_id, application_title,total_score) 
			values(null,#{application_title},null)
	</insert>
	<insert id="insFile" parameterType="file">
		insert into filestorage (fname, path, Participant_id) values(#{fname},#{path}, #{participant_id})
	</insert>
</mapper>